[
  {rabbit, [
    {tcp_listeners, [5672]},
    {ssl_listeners, [5671]},
    {ssl_options, [
          {cacertfile, "/etc/rabbitmq/ca-cert.pem"},
          {certfile, "/etc/rabbitmq/server-cert.pem"},
          {keyfile, "/etc/rabbitmq/server-key.pem"},
          {verify, verify_peer},
          {fail_if_no_peer_cert, true}
    ]}
  ]}
].
